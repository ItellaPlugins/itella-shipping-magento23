<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <container name="listing_top">
        <massaction name="listing_massaction">
            <action name="mass_labels">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">mass_labels</item>
                        <item name="label" xsi:type="string" translate="true">Generate Smartpost Labels</item>
                        <item name="url" xsi:type="url" path="itella/order/printMassLabels"/>
                    </item>
                </argument>
            </action>
            <action name="mass_manifest">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">mass_manifest</item>
                        <item name="label" xsi:type="string" translate="true">Generate Smartpost Manifest</item>
                        <item name="url" xsi:type="url" path="itella/order/printMassManifest"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </container>
</listing>